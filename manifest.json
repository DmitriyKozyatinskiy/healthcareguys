{
  "manifest_version": 2,
  "icons": {
    "16": "img/icons/icon16.png",
    "32": "img/icons/icon32.png",
    "48": "img/icons/icon48.png",
    "128": "img/icons/icon128.png"
  },
  "name": "Digital Health News",
  "short_name": "HealthCare Guys",
  "description": "HealthCare Guys",
  "version": "2.0.6",
  "background": {
    "scripts": [
      "js/libs/jquery.js",
      "js/background.js"
    ]
  },
  "browser_action": {
    "default_popup": "popup.html"
  },
  "permissions": [
    "cookies",
    "storage",
    "activeTab"
  ],
  "content_scripts": [{
    "matches": [
      "<all_urls>"
    ],
    "js": [
      "js/libs/jquery.js",
      "js/dataProvider.js",
      "js/urlValidator.js"
    ]
  }],
  "options_ui": {    
    "page": "options.html",    
    "chrome_style": true 
  }
}
