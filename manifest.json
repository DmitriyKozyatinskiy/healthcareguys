{
  "manifest_version": 2,
  "icons": {
    "16": "img/icons/cropped-faviocn-32x32.png",
    "48": "img/icons/cropped-faviocn-32x32.png",
    "128": "img/icons/cropped-faviocn-32x32.png"
  },
  "name": "HealthCareGuys",
  "description": "HealthCareGuys",
  "version": "1.0",
  "background": {
    "scripts": [ "js/background.js" ]
  },
  "browser_action": {
    "default_popup": "popup.html"
  },
  "permissions": [
    "cookies",
    "storage",
    "activeTab"
  ],
  "content_scripts": [{
    "matches": [
      "<all_urls>"
    ],
    "js": [
      "js/libs/jquery.js",
      "js/dataProvider.js"
    ]
  }]
}
