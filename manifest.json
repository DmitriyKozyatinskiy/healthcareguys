{
  "manifest_version": 2,
  "icons": {
    "16": "img/icons/icon16.png",
    "32": "img/icons/icon32.png",
    "48": "img/icons/icon48.png",
    "128": "img/icons/icon128.png"
  },
  "name": "Digital Health News",
  "short_name": "HealthCare Guys",
  "description": "HealthCare Guys",
  "version": "2.0.8",
  "background": {
    "scripts": [
      "js/libs/jquery.js",
      "js/background.js",
      "js/installHandler.js"
    ]
  },
  "browser_action": {
    "default_popup": "popup.html"
  },
  "permissions": [
    "tabs",
    "cookies",
    "storage",
    "activeTab",
    "http://*/",
    "https://*/"
  ],
  "content_scripts": [{
    "matches": [
      "<all_urls>"
    ],
    "js": [
      "js/libs/jquery.js",
      "js/dataProvider.js",
      "js/urlValidator.js",
	  "js/notification.js"      
    ],
	"run_at": "document_end"
  }], 
  "options_ui": {    
    "page": "options.html",    
    "chrome_style": true 
  }
}
